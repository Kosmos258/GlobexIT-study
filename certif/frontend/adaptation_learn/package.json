{
  "name": "frontend-preset",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc && vite build && npx dprint fmt",
    "build-no-lint": "tsc && vite build && npx dprint fmt",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && npx dprint fmt",
    "preview": "vite preview",
    "deploy": "node deployer/deploy.js",
    "build:delivery": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc && vite build && node deployer/deploy.js && npx dprint fmt"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.10.1",
    "antd": "^5.29.1",
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "sass": "^1.94.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "dprint": "^0.50.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
